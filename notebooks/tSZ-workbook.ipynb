   "source": [
    "import numpy as np\n",
    "\n",
    "h = 6.626070e-34 # J / sec\n",
    "t_cmb = 2.726 # Kelvin, Planck\n",
    "kb = 1.380649e-23 # J / K\n",
    "freqs = np.array([90., 150.]) * 1e9 # GHz\n",
    "\n",
    "def f_tsz(nu):\n",
    "    x = h * nu / t_cmb / kb\n",
    "    return (x / np.tanh(x/2)) - 4\n",
    "          \n",
    "print(f_tsz(freqs))\n",
    "\n",
